{
  "expo": {
    "name": "solarcalc",
    "slug": "solarcalc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/vai.jpeg",
    "scheme": "solarcalc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/vai.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
